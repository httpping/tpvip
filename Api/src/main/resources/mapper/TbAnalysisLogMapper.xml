<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tp.api.mapper.TbAnalysisLogMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tp.api.entity.TbAnalysisLog">
        <id column="id" property="id" />
        <result column="domain" property="domain" />
        <result column="count" property="count" />
        <result column="phone" property="phone" />
        <result column="update_date" property="updateDate" />
        <result column="day" property="day" />
        <result column="hour" property="hour" />
        <result column="month" property="month" />
        <result column="month_day" property="monthDay" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, domain, count, phone, update_date, day, hour, month, month_day
    </sql>

</mapper>
